{
  "name": "dearest-justin",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Kelly Mears",
    "email": "hello@kellymears.me"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0-beta.3",
    "jquery": "^1.9.1",
    "query-string": "^5.1.0",
    "react": "^16.2.0",
    "react-anime": "^2.1.0",
    "react-countup": "^3.0.2",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "react-player": "^1.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "react-stripe-checkout": "^2.6.3",
    "react-stripe-elements": "^1.4.0",
    "react-svg": "^2.2.10",
    "reactstrap": "^5.0.0-alpha.4",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "lint": "./node_modules/.bin/eslint ./ --fix",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true,
        "arrowFunctions": true,
        "classes": true
      }
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "strict": 0,
      "comma-dangle": [
        1,
        "never"
      ],
      "semi": [
        1,
        "never"
      ],
      "no-cond-assign": [
        1,
        "always"
      ],
      "no-constant-condition": 1,
      "no-control-regex": 1,
      "no-debugger": 1,
      "no-dupe-args": 1,
      "no-dupe-keys": 1,
      "no-duplicate-case": 1,
      "no-empty-character-class": 1,
      "no-empty": 1,
      "no-ex-assign": 1,
      "no-extra-boolean-cast": 1,
      "no-extra-semi": 1,
      "no-func-assign": 1,
      "no-inner-declarations": [
        1,
        "both"
      ],
      "no-invalid-regexp": 1,
      "no-irregular-whitespace": 1,
      "no-negated-in-lhs": 1,
      "no-obj-calls": 1,
      "no-regex-spaces": 1,
      "no-sparse-arrays": 1,
      "no-unreachable": 1,
      "use-isnan": 1,
      "valid-typeof": 1,
      "accessor-pairs": 1,
      "complexity": [
        1,
        10
      ],
      "curly": [
        1,
        "multi"
      ],
      "default-case": 1,
      "dot-notation": 1,
      "dot-location": [
        1,
        "property"
      ],
      "no-alert": 0,
      "no-labels": 1,
      "no-eq-null": 1,
      "no-eval": 1,
      "no-extend-native": 1,
      "no-extra-bind": 1,
      "no-fallthrough": 1,
      "no-floating-decimal": 1,
      "no-invalid-this": 0,
      "no-lone-blocks": 1,
      "no-loop-func": 1,
      "no-multi-str": 1,
      "no-native-reassign": 1,
      "no-new-func": 1,
      "no-new-wrappers": 1,
      "no-octal-escape": 1,
      "no-octal": 1,
      "no-redeclare": 1,
      "no-return-assign": 1,
      "no-script-url": 1,
      "no-self-compare": 1,
      "no-sequences": 1,
      "yoda": [
        1,
        "never",
        {}
      ],
      "no-delete-var": 1,
      "no-undef": 1,
      "no-unused-vars": 1,
      "callback-return": 1,
      "array-bracket-spacing": [
        1,
        "never",
        {}
      ],
      "brace-style": [
        1,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "camelcase": [
        1,
        {
          "properties": "always"
        }
      ],
      "comma-spacing": 1,
      "comma-style": 1,
      "computed-property-spacing": 1,
      "eol-last": 1,
      "indent": [
        2,
        2
      ],
      "key-spacing": [
        1,
        {
          "align": "value"
        }
      ],
      "lines-around-comment": [
        1,
        {
          "beforeBlockComment": true
        }
      ],
      "no-mixed-spaces-and-tabs": 1,
      "no-multiple-empty-lines": [
        1,
        {
          "max": 1
        }
      ],
      "object-curly-spacing": [
        1,
        "never",
        {}
      ],
      "padded-blocks": [
        1,
        "never"
      ],
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "space-in-parens": [
        2,
        "never"
      ],
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error"
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-import": "^1.6.3",
    "eslint": "^4.1.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "react-a11y": "^1.0.0",
    "source-map-explorer": "^1.5.0",
    "svg-react-loader": "^0.4.5"
  }
}
